<%@ page contentType="text/html;charset=UTF-8" language="java" %><html><head>    <title></title>    <link rel="stylesheet" href="bootstrap3.3.5/css/bootstrap.min.css">    <link rel="stylesheet" href="bootstrap3.3.5/plugins/table/bootstrap-table.css">    <link href="bootstrap3.3.5/plugins/page/jBootsrapPage.css" rel="stylesheet"/>    <script src="js/jquery-1.9.1.min.js"></script>    <script src="bootstrap3.3.5/js/bootstrap.min.js"></script>    <script src="bootstrap3.3.5/plugins/table/bootstrap-table.js"></script>    <script src="bootstrap3.3.5/plugins/table/locale/bootstrap-table-zh-CN.js"></script>    <script src="bootstrap3.3.5/plugins/page/jBootstrapPage.js"></script></head><body><div style="margin: 20px 20px;height: 300px;">    <table id="table"></table>    <ul class="pagination"></ul></div></body><script type="application/javascript">    $(function () {        $('#table').bootstrapTable({            columns: [{                field: 'id',                title: 'Item ID'            }, {                field: 'name',                title: 'Item Name'            }, {                field: 'price',                title: 'Item Price'            }],            data: [{                id: 1,                name: 'Item 1',                price: '$1'            }, {                id: 2,                name: 'Item 2',                price: '$2'            }]        });        createPage(10, 15, 150);        function createPage(pageSize, buttons, total) {            $(".pagination").jBootstrapPage({                pageSize: pageSize,                total: total,                maxPageButton: buttons,                onPageClicked: function (obj, pageIndex) {                    alert((pageIndex + 1) + 'é¡µ');                }            });        }    });</script></html>