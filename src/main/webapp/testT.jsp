<%@ page import="com.app.mvc.business.service.TestService" %><%@ page import="com.app.mvc.common.SpringHelper" %><%@ page import="java.lang.reflect.Field" %><%@ page import="java.util.concurrent.ConcurrentHashMap" %><%@ page import="java.util.concurrent.ThreadPoolExecutor" %><%@ page contentType="text/html;charset=UTF-8" language="java" %><html><head>    <title>Title</title></head><body><%    // 启动服务后,先运行/test/test.json, 再打开这个jsp才能达到测试的目的    TestService testService = SpringHelper.popBean(TestService.class);    Class clazz = testService.getClass();    Field m = clazz.getDeclaredField("threadPoolExecutorMap");    m.setAccessible(true);    m.set(testService, new ConcurrentHashMap<String, ThreadPoolExecutor>());%></body></html>